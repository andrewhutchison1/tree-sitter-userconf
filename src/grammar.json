{
  "name": "userconf",
  "rules": {
    "document": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_record_body"
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "_separator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "unquoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "record"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        }
      ]
    },
    "record": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_record_body"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_record_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "record_entry"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_separator"
              },
              {
                "type": "SYMBOL",
                "name": "record_entry"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_separator"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "record_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "quoted_string"
              },
              {
                "type": "SYMBOL",
                "name": "unquoted_string"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_list_body"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "_list_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_value"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_separator"
              },
              {
                "type": "SYMBOL",
                "name": "_value"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_separator"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "unquoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\s\\[\\]\\{\\}\";,>]+"
      }
    },
    "quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\"\\\\]"
                },
                {
                  "type": "PATTERN",
                  "value": "\\\\(\"|\\\\|0|[a-z]|(x[0-9a-fA-F]{2})|(u[0-9a-fA-F]{6}))"
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": ";"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

